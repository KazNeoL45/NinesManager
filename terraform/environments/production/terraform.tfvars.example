aws_region         = "us-east-1"
environment        = "production"
project_name       = "ninesmanager"
vpc_cidr          = "10.0.0.0/16"
availability_zones = ["us-east-1a", "us-east-1b", "us-east-1c"]

instance_type     = "t3.medium"
min_size         = 2
max_size         = 10
desired_capacity = 2

db_instance_class    = "db.t3.medium"
db_allocated_storage = 100
db_name             = "ninesmanager_production"
db_username         = "ninesmanager"
db_password         = "CHANGE-ME-SECURE-PASSWORD"

redis_node_type       = "cache.t3.medium"
redis_num_cache_nodes = 2

domain_name = "ninesmanager.com"

ssl_certificate_arn = "arn:aws:acm:us-east-1:123456789012:certificate/CHANGE-ME"

allowed_cidr_blocks = ["0.0.0.0/0"]
